<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:drawable/picture_frame">

    <TextView
        android:text="Aicte ID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView43"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <TextView
        android:text="Institution Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView43"
        android:id="@+id/textView47"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <TextView
        android:text="Institution Type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView47"
        android:id="@+id/textView48"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <TextView
        android:text="Address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView48"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView49"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <TextView
        android:text="State"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView49"
        android:id="@+id/textView50"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <TextView
        android:text="District"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView50"
        android:id="@+id/textView51"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <TextView
        android:text="City"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView51"
        android:id="@+id/textView52"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView47"
        android:layout_toEndOf="@+id/textView47"
        android:id="@+id/x_label_closed_institutes_aicteid" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView47"
        android:layout_toRightOf="@+id/textView47"
        android:layout_toEndOf="@+id/textView47"
        android:id="@+id/x_label_closed_institutes_institution_name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView47"
        android:layout_toRightOf="@+id/textView47"
        android:layout_toEndOf="@+id/textView47"
        android:id="@+id/x_label_closed_institutes_institution_type" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView48"
        android:layout_alignLeft="@+id/x_label_closed_institutes_institution_type"
        android:layout_alignStart="@+id/x_label_closed_institutes_institution_type"
        android:id="@+id/x_label_closed_institutes_address" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView49"
        android:layout_alignLeft="@+id/x_label_closed_institutes_address"
        android:layout_alignStart="@+id/x_label_closed_institutes_address"
        android:id="@+id/x_label_closed_institutes_state" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/x_label_closed_institutes_state"
        android:layout_alignRight="@+id/x_label_closed_institutes_state"
        android:layout_alignEnd="@+id/x_label_closed_institutes_state"
        android:id="@+id/x_label_closed_institutes_district" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView51"
        android:layout_alignLeft="@+id/x_label_closed_institutes_district"
        android:layout_alignStart="@+id/x_label_closed_institutes_district"
        android:id="@+id/x_label_closed_institutes_city" />
</RelativeLayout>