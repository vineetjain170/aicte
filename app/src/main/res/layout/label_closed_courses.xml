<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:drawable/picture_frame"
    android:layout_margin="5dp">

    <!--
        Label_closed courses
    -->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Institute Name"
        android:layout_margin="10dp"
        android:id="@+id/x_label_closed_courses_institution_name"
        android:textAppearance="@android:style/TextAppearance.Material" />
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#dedede"
        android:layout_margin="10dp"/>



    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AICTE ID"
            android:layout_marginLeft="10dp"
            android:textAppearance="@android:style/TextAppearance.Material" />

        <TextView
            android:id="@+id/x_label_closed_courses_aicteid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:text="set id"
            android:textAppearance="@android:style/TextAppearance.Material" />
    </LinearLayout>



    <LinearLayout
        android:orientation="horizontal"
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Institute"
            android:layout_marginLeft="10dp"
            android:textAppearance="@android:style/TextAppearance.Material" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="set institute"
            android:layout_marginLeft="54dp"
            android:id="@+id/x_label_closed_courses_institution_type"
            android:textAppearance="@android:style/TextAppearance.Material" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="State"
            android:textAppearance="@android:style/TextAppearance.Material" />

        <TextView
            android:id="@+id/x_label_closed_courses_state"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="78dp"
            android:text="set state"
            android:textAppearance="@android:style/TextAppearance.Material" />
    </LinearLayout>


    <LinearLayout
        android:orientation="horizontal"
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="District"
            android:layout_marginLeft="10dp"
            android:textAppearance="@android:style/TextAppearance.Material" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="set district"
            android:layout_marginLeft="62dp"
            android:id="@+id/x_label_closed_courses_district"
            android:textAppearance="@android:style/TextAppearance.Material" />
    </LinearLayout>



    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Course ID"
            android:layout_marginLeft="10dp"
            android:textAppearance="@android:style/TextAppearance.Material" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="set id"
            android:layout_marginLeft="43dp"
            android:id="@+id/x_label_closed_courses_courseid"
            android:textAppearance="@android:style/TextAppearance.Material" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="University"
            android:textAppearance="@android:style/TextAppearance.Material" />

        <TextView
            android:id="@+id/x_label_closed_courses_university"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="41dp"
            android:text="set university"
            android:textAppearance="@android:style/TextAppearance.Material" />
    </LinearLayout>


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Level"
            android:layout_marginLeft="10dp"
            android:textAppearance="@android:style/TextAppearance.Material" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="set level"
            android:layout_marginLeft="77dp"
            android:id="@+id/x_label_closed_courses_level"
            android:textAppearance="@android:style/TextAppearance.Material" />
    </LinearLayout>



    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Course"
            android:layout_marginLeft="10dp"
            android:textAppearance="@android:style/TextAppearance.Material" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="set course"
            android:layout_marginLeft="63dp"
            android:id="@+id/x_label_closed_courses_course"
            android:textAppearance="@android:style/TextAppearance.Material" />
    </LinearLayout>



    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Shift"
            android:layout_marginLeft="10dp"
            android:textAppearance="@android:style/TextAppearance.Material" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="set shift"
            android:layout_marginLeft="81dp"
            android:id="@+id/x_label_closed_courses_shift"
            android:textAppearance="@android:style/TextAppearance.Material" />
    </LinearLayout>



    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Full/Part Time"
            android:layout_marginLeft="10dp"
            android:textAppearance="@android:style/TextAppearance.Material" />

        <TextView
            android:id="@+id/x_label_closed_courses_full_part_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            android:text="set id"
            android:textAppearance="@android:style/TextAppearance.Material" />
    </LinearLayout>




</LinearLayout>

